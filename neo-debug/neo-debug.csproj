<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>NewEconoLabs</Authors>
    <Company>NewEconoLabs</Company>
    <PackageId>neo-debug</PackageId>
    <Product>neo-debug</Product>
    <Description>修改了官方的neo，增加了debug的功能。能够输出虚拟机执行智能合约时的指令集，以便调试。增加了block入库，以及对leveldb的操作入库。</Description>
    <Copyright>MIT</Copyright>
    <AssemblyName>neo-debug</AssemblyName>
    <RepositoryUrl>https://github.com/NewEconoLab/Neo_SDK_Thin</RepositoryUrl>
    <PackageProjectUrl>https://nel.group/</PackageProjectUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>neo debug</PackageTags>
    <FileVersion>2.10.2</FileVersion>
    <Version>2.10.2</Version>
    <PackageReleaseNotes>update info</PackageReleaseNotes>
    <PackageIconUrl>https://nel-logos.oss-cn-hangzhou.aliyuncs.com/NEL_logo_256.png</PackageIconUrl>
    <dependencies>
      <dependency id="Akka" version="1.3.11" />
      <dependency id="Microsoft.AspNetCore.ResponseCompression" version="2.2.0" />
      <dependency id="Microsoft.AspNetCore.Server.Kestrel" version="2.2.0" />
      <dependency id="Microsoft.AspNetCore.Server.Kestrel.Https" version="2.2.0" />
      <dependency id="Microsoft.AspNetCore.WebSockets" version="2.2.0" />
      <dependency id="Microsoft.EntityFrameworkCore.Sqlite" version="2.1.4" />
      <dependency id="Microsoft.Extensions.Configuration.Json" version="2.2.0" />
      <dependency id="NEL.LLVM" version="1.0.0" />
      <dependency id="NEL.Simple.SDK" version="1.0.5" />
      <dependency id="neo-vm-debug" version="2.4.3" />
      <dependency id="System.Text.Encodings.Web" version="4.5.0" />
    </dependencies>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="MongoHelper.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Akka" Version="1.3.11" />
    <PackageReference Include="Microsoft.AspNetCore.ResponseCompression" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel.Https" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.WebSockets" Version="2.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="2.1.4" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="2.2.0" />
    <PackageReference Include="NEL.LLVM" Version="1.0.0" />
    <PackageReference Include="NEL.Simple.SDK" Version="1.0.5" />
    <PackageReference Include="neo-vm-debug" Version="2.4.3" />
    <PackageReference Include="System.Text.Encodings.Web" Version="4.5.0" />
  </ItemGroup>

</Project>
